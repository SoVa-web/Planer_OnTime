{
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "../.eslintrc.json"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "rules": {
        "linebreak-style": "off",
        "semi":"off",
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-plusplus": "off",
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
        "object-curly-newline":"off",
        "react/jsx-one-expression-per-line": "off"
    }
}