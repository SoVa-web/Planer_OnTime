{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
       //"prettier",
        "airbnb-base",
        "../.eslintrc.json"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
   //     "prettier/prettier": ["error", {"singleQuote": true}],
        "no-plusplus": "off",
        "no-tabs": "off",
        "no-alert": "off",
        "no-unused-vars": "warn",
        "max-len": ["error", { "code": 80 }],
        "indent": ["off", "tab"]
    }
}