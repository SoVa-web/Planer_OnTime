{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "../.eslintrc.json"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-plusplus": "off",
        "no-tabs": "off",
        "no-alert": "off",
        "no-unused-vars": "warn",
        "max-len": ["error", { "code": 80 }],
        "indent": ["off", "tab"],
        "operator-linebreak":"off"
    }
}